{
    "Statement": [
        {
            "Effect": "Allow",
            "Action": ["s3:ListBucket"],
            "Resource": ["arn:aws:s3:::shipbuilder"]
        },
        {
            "Effect": "Allow",
            "Action": [
				"s3:PutObject",
				"s3:GetObject",
				"s3:DeleteObject"
			],
            "Resource": ["arn:aws:s3:::shipbuilder/*"]
        }
    ]
}
